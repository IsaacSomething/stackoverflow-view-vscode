{
  "name": "stackoverflow-view",
  "displayName": "stackoverflow-view",
  "description": "Integrated solution for searching and finding answers in stackoverflow",
  "version": "0.0.1",
  "homepage": "https://github.com/IsaacSomething/stackoverflow-view-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/IsaacSomething/stackoverflow-view-vscode"
  },
  "author": {
    "name": "Tim"
  },
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Stack Overflow",
    "StackOverflow",
    "Answers",
    "Questions"
  ],
  "activationEvents": [
    "onCommand:extension.searchStackoverflow",
    "onCommand:extension.topPickStackoverflow"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.searchStackoverflow",
        "title": "Stackoverflow: Search"
      },
      {
        "command": "extension.topPickStackoverflow",
        "title": "Stackoverflow: Top Pick Questions"
      }
    ],
    "configuration": [
      {
        "title": "Stackoverflow View",
        "properties": {
          "stackoverflow.view.language": {
            "type": "string",
            "enum": [
              "English",
              "Spanish",
              "Russian",
              "Portuguese",
              "Japanese"
            ],
            "default": "English",
            "description": "Select your prefered language for Stackoverflow",
            "scope": "window"
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "extension.searchStackoverflow",
        "key": "ctrl+alt+s",
        "mac": "ctrl+cmd+s"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.0-next.0"
  }
}